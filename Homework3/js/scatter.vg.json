{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "width": 800,
    "height": 600,
    "title": "COVID-19 Cases per Country (13 Oct 2020)",
    "data": {
      "url": "https://raw.githubusercontent.com/enxinwww/FIT3179/main/Homework3/data/Happiness2015.csv"
    },
    "params": [
      {
        "name": "Continent_selection",
        "bind": {
          "input": "select",
          "options": [
            null,
            "North America",
            "South America",
            "Europe",
            "Africa",
            "Asia",
            "Oceania"
          ],
          "labels": [
            "Show All",
            "North America",
            "South America",
            "Europe",
            "Africa",
            "Asia",
            "Oceania"
          ],
          "name": "Continent Selection: "
        }
      }
    ],
    "encoding": {
      "x": {
        "field": "Economy (GDP per Capita)",
        "type": "quantitative",
        "title": "Economy (GDP per Capita)",
        "axis": {"tickCount": 7},
        "scale": {"type": "log", "domain": [1, 10]}
      },
      "y": {
        "field": "Happiness Score",
        "type": "quantitative",
        "axis": {"tickCount": 6},
        "scale": {"type": "log", "domain": [1, 10]}
      }
    }
  }
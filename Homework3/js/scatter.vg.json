{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "width": "container",
  "title": "COVID-19 Cases per Country (13 Oct 2020)",
  "data": {
    "url": "Happiness2015.csv"
},
    "params": [
    {
      "name": "Population_Above",

      "value": 0,
      "bind": {"input": "range", "min": 0, "max": 100000000, "step": 1000000}
    }
  ],
  "selection": {
    "continent_selection": {
      "type": "multi", "fields": ["Continent"], "bind": "legend"
    }
  },
  "mark": "point",
  "encoding": {
    "x": {
      "field": "Economy (GDP per Capita)",
      "type": "quantitative",
      "title":"Economy (GDP per Capita)",
      "axis": {"tickCount": 7},
      "scale": {"type": "log",  "domain":[1, 10000000]}
    },
    "y": {
      "field": "Happiness Score",
      "type": "quantitative",
      "axis": { "tickCount": 6},
      "scale": {"type": "log", "domain":[1, 1000000]}
    }
}
}
